@article{kudatarkar2015survey,
title={A Survey on Unstructured Text Analytics Approaches for Qualitative Evaluation of Resumes},
author={Kudatarkar, Vinaya Ramesh and Ramannavar, Manjula and Sidnal, Dr Nandini S},
