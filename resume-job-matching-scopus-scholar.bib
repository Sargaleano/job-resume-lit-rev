@CONFERENCE{Bian202065,
author={Bian, S. and Chen, X. and Zhao, W.X. and Zhou, K. and Hou, Y. and Song, Y. and Zhang, T. and Wen, J.-R.},
title={Learning to Match Jobs with Resumes from Sparse Interaction Data using Multi-View Co-Teaching Network},
journal={International Conference on Information and Knowledge Management, Proceedings},
year={2020},
