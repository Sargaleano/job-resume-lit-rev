@ARTICLE{Maree2019713,
author={Maree, M. and Kmail, A.B. and Belkhatir, M.},
title={Analysis and shortcomings of e-recruitment systems: Towards a semantics-based approach addressing knowledge incompleteness and limited domain coverage},
journal={Journal of Information Science},
year={2019},
